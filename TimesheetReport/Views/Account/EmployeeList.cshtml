@using PagedList.Mvc
@model PagedList.IPagedList<TimesheetReport.WebUI.ViewModels.Employee.Employee>
<table class="table table-striped">
    <tr>
        <th>
            @Html.ActionLink("FirstName", "Index")
        </th>
        <th>
            @Html.ActionLink("LastName", "Index")
        <th>
            @Html.ActionLink("Genders", "Index")
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Genders)
            </td>
        </tr>
    }
</table>
@Html.PagedListPager(Model, page => Url.Action("GetListEmployee", new { page }))
